{
  "name": "matai-pro",
  "version": "0.0.0-NotUsed",
  "scripts": {
    "build": "ng build --prod --base-href=/matai-pro-doc/",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s --pkg components/package.json",
    "e2e": "ng e2e",
    "generate": "bash ./build.sh",
    "integration-cli": "npm run generate && cd integration/angular-cli && npm run integration",
    "lint": "tslint -c tslint.json 'components/*/*.ts'",
    "start": "ng serve --port 4202",
    "release": "node ./scripts/publish.js",
    "test": "ng test --watch=false --code-coverage",
    "move-doc": "cp -r ./dist/matai-pro/* ../matai-pro-doc/ && cd ../matai-pro-doc/ && git add . && git commit -m 'update: docs' && git push"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "core-js": "^2.5.4",
    "ng-zorro-antd": "^1.8.0",
    "rxjs": "~6.2.0",
    "three": "^0.98.0",
    "three-orbit-controls": "^82.1.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.8.0",
    "@angular-devkit/core": "^0.6.0",
    "@angular/cli": "~6.2.3",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@types/three": "^0.93.10",
    "chalk": "^2.4.1",
    "child_process": "^1.0.2",
    "codelyzer": "~4.3.0",
    "fs-extra": "^7.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "prismjs": "^1.15.0",
    "protractor": "~5.4.0",
    "readline-sync": "^1.4.9",
    "rollup": "^0.49.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-uglify": "^2.0.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~2.9.2"
  }
}
